	dependency	"$S/contrib/dev/wpi/iwlwifi-3945-2.14.4.fw.uu"					\
	compile-with	"uudecode -o ${.TARGET} $S/contrib/dev/wpi/iwlwifi-3945-2.14.4.fw.uu"	\
	no-obj no-implicit-rule						\
	clean		"wpi.fw"
